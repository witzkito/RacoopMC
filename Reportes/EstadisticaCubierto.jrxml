	<queryString><![CDATA[SELECT
	Socios.nroSocio,
	Socios.dia_Ingreso as dia,
	Socios.mes_Ingreso as mes,
	Socios.anio_Ingreso as anio,
	Socios.nomApe,
	Socios.tipoDoc,
	Socios.nroIden,
	Sum(CensoPlantacionDetalles.hectareas) as hectareas
FROM
	Socios, CensoPlantacionDetalles, CensosPlantaciones
WHERE
	Socios.nroSocio = CensosPlantaciones.nroSocio
	and
	CensosPlantaciones.temporada LIKE $P{temporada}
	and
	CensosPlantaciones.numero = CensoPlantacionDetalles.nroCensoPlantacion
	and
	CensoPlantacionDetalles.cubierto = $P{cubierto}	
	and
	CensoPlantacionDetalles.nroSocio = Socios.nroSocio
GROUP BY
	Socios.nroSocio,
	Socios.dia_Ingreso,
	Socios.mes_Ingreso,
	Socios.anio_Ingreso,
	Socios.nomApe,
	Socios.tipoDoc,
	Socios.nroIden]]></queryString>

